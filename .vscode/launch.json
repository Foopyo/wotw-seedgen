{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach executable 'seed_gen_cli'",
            "pid": "${command:pickMyProcess}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'seed_gen_cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=seed_gen_cli",
                    "--package=seed_gen_cli"
                ],
                "filter": {
                    "kind": "bin"
                }
            },
            "args": [
                "seed",
                "C:\\moon\\test",
                "--areas",
                "C:\\moon\\areas.wotw",
                "--locations",
                "C:\\moon\\loc_data.csv",
                "--debug",
                "--validate",
                "--gen_flags",
                "moki"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'seed_gen_cli'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib=seed_gen_cli",
                ],
                "filter": {
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}